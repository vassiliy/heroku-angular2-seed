{
  "name": "heroku-angular2-seed",
  "version": "1.0.0",
  "description": "A simple starter Angular2 project ready to deploy to Heroku",
  "scripts": {
    "build": "NODE_ENV=production node node_modules/.bin/webpack && cp src/index.html dist/index.html",
    "postinstall": "npm run build",
    "server": "webpack-dev-server --inline --colors --progress --display-error-details --display-cached --port 3000  --content-base src",
    "start": "npm run server"
  },
  "contributors": [
    "Rob Wormald <robwormald@gmail.com>",
    "PatrickJS <github@gdi2290.com>",
    "Vassiliy Pimkin <vassiliy.pimkin@gmail.com"
  ],
  "license": "MIT",
  "devDependencies": {
    "dotenv": "^2.0.0"
  },
  "dependencies": {
    "@angular/common": "^2.2.4",
    "@angular/compiler": "^2.2.4",
    "@angular/core": "^2.2.4",
    "@angular/forms": "^2.2.4",
    "@angular/http": "^2.2.4",
    "@angular/platform-browser": "^2.2.4",
    "@angular/platform-browser-dynamic": "^2.2.4",
    "@angular/platform-server": "^2.2.4",
    "@angular/router": "^3.2.4",
    "@angular/upgrade": "^2.2.4",
    "@types/core-js": "^0.9.35",
    "@types/node": "^6.0.50",
    "angular2-in-memory-web-api": "^0.0.21",
    "angular2-template-loader": "^0.6.0",
    "awesome-typescript-loader": "^3.0.0-beta.9",
    "bootstrap": "^3.3.7",
    "core-js": "^2.4.1",
    "css-loader": "^0.26.1",
    "ie-shim": "^0.1.0",
    "raw-loader": "^0.5.1",
    "reflect-metadata": "^0.1.3",
    "rxjs": "5.0.0-beta.12",
    "to-string-loader": "^1.1.4",
    "typescript": "2.0.10",
    "webpack": "^1.13.3",
    "webpack-dev-server": "^1.14.0",
    "webpack-merge": "^1.0.2",
    "zone.js": "^0.6.26"
  },
  "keywords": [
    "Angular2",
    "angular2-seed",
    "Heroku"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vassiliy/heroku-angular2-seed.git"
  },
  "bugs": {
